<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE install PUBLIC "-//Joomla! 2.5//DTD template 1.0//EN" "http://www.joomla.org/xml/dtd/2.5/template-install.dtd">
<extension version="2.5" type="template" client="site">
  <name>Helleshaus Template</name>
  <version>1.0</version>
  <creationDate>04/05/2013</creationDate>
  <author>Elena Fedorova/Mikhail Vnukov</author>
  <authorEmail>mikhail.vnukov@gmail.com</authorEmail>
  <copyright>Copyright (C) 2012 Elena Fedorova/Mikhail Vnukov</copyright>
  <description>Helleshaus template</description>
  <files>
    <filename>index.php</filename>
    <folder>css</folder>
    <folder>js</folder>
    <folder>images</folder>
  </files>
  <positions>
        <position>debug</position>
        <position>header</position>
        <position>footer</position>
        <position>right-sidebar</position>

        <position>order-form</position>
        <position>feedback</position>
        <position>actions</position>
        <position>clients-footer</position>
        <position>services</position>
  </positions>
  <config>
    <fields name="params">
      <fieldset name="advanced">
        <field name="logo" type="media" label="Логотип" description="Логотип" />
      </fieldset>
    </fields>
  </config>
</extension>
